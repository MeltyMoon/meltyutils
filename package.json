	"description": "Various Utilities",
	"main": "dist/meltyutils.js",
	"module": "dist/meltyutils.esm.js",
	"author": "MeltyMoon",
	"license": "MIT",
	"private": false,
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"eslint": "^8.35.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^3.18.0"
	},
	"scripts": {
		"js-compile": "npm-run-all --aggregate-output --parallel js-compile-*",
		"js-compile-standalone": "rollup --environment ESM:false --config build/rollup.config.js --sourcemap",
		"js-compile-standalone-esm": "rollup --environment ESM:true --config build/rollup.config.js --sourcemap"
	}
